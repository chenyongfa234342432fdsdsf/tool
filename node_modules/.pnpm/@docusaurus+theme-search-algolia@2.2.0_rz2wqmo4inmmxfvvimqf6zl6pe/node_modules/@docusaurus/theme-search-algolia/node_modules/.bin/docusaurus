#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@docusaurus+core@2.2.0_5qmyyve7pk7d54o5btj5hiqczy/node_modules/@docusaurus/core/bin/docusaurus.mjs" "$@"
else
  exec node  "$basedir/../../../../../../@docusaurus+core@2.2.0_5qmyyve7pk7d54o5btj5hiqczy/node_modules/@docusaurus/core/bin/docusaurus.mjs" "$@"
fi
