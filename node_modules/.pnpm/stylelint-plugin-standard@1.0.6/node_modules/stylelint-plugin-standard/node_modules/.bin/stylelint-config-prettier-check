#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint-config-prettier@9.0.3_stylelint@14.8.5/node_modules/stylelint-config-prettier/bin/check.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint-config-prettier@9.0.3_stylelint@14.8.5/node_modules/stylelint-config-prettier/bin/check.js" "$@"
fi
