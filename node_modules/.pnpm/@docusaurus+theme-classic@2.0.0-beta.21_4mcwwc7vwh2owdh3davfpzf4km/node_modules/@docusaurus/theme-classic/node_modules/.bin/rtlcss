#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../rtlcss@3.5.0/node_modules/rtlcss/bin/rtlcss.js" "$@"
else
  exec node  "$basedir/../../../../../../rtlcss@3.5.0/node_modules/rtlcss/bin/rtlcss.js" "$@"
fi
