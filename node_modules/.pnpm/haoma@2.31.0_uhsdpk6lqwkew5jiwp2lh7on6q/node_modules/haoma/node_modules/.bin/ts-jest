#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-jest@26.5.6_xuote2qreek47x2di7kesslrai/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../../ts-jest@26.5.6_xuote2qreek47x2di7kesslrai/node_modules/ts-jest/cli.js" "$@"
fi
