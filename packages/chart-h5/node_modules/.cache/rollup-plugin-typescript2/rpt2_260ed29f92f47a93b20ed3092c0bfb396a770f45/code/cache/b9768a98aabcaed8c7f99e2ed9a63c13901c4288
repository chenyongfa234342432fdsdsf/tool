{"code":"import React, { useEffect, useRef } from 'react';\r\nimport { createChart } from 'lightweight-charts';\r\nfunction DeptChart(props) {\r\n    const deptRef = useRef(null);\r\n    const chartRef = useRef(null);\r\n    useEffect(() => {\r\n        const chart = createChart(document.getElementById('chart'), {\r\n            layout: props.chartLayoutOptions,\r\n            // 时间刻度和价格刻度\r\n            timeScale: {\r\n                visible: false,\r\n            },\r\n            rightPriceScale: {\r\n                visible: false,\r\n            },\r\n            // 布局相关\r\n            crosshair: {\r\n                horzLine: {\r\n                    visible: false,\r\n                    labelVisible: false,\r\n                },\r\n                vertLine: {\r\n                    visible: false,\r\n                    labelVisible: false,\r\n                },\r\n            },\r\n            grid: {\r\n                vertLines: {\r\n                    visible: false,\r\n                },\r\n                horzLines: {\r\n                    visible: false,\r\n                },\r\n            },\r\n        });\r\n        const front = props.data[props.data.length - 1]?.value || 0;\r\n        const end = props.data[props.data.length - 2]?.value || 0;\r\n        const isUp = front - end > 0;\r\n        chartRef.current = chart.addAreaSeries({\r\n            lineColor: isUp ? props.createChart.upColor : props.createChart.downColor,\r\n            topColor: isUp ? props.createChart.upColor : props.createChart.downColor,\r\n            bottomColor: isUp ? props.createChart.upLightColor : props.createChart.downLightColor,\r\n            crosshairMarkerVisible: false,\r\n            priceLineVisible: false,\r\n        });\r\n        chartRef.current.setData(props.data);\r\n        chart.timeScale().fitContent();\r\n    }, []);\r\n    return React.createElement(\"div\", { ref: deptRef, className: \"chart\", id: \"chart\" });\r\n}\r\nDeptChart.displayName = 'deptChart';\r\nexport default DeptChart;\r\n//# sourceMappingURL=dept-chart.js.map","references":["/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/react@17.0.2/node_modules/react/index.js","/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/lightweight-charts@3.8.0/node_modules/lightweight-charts/dist/typings.d.ts","/Users/xiaoyu/Desktop/xiangmu/toolbox/node_modules/.pnpm/@nbit+chart-utils@1.0.0-beta.35/node_modules/@nbit/chart-utils/lib/index.d.ts"],"map":"{\"version\":3,\"file\":\"dept-chart.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/components/dept/dept-chart.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAE,SAAS,EAAE,MAAM,EAAE,MAAM,OAAO,CAAA;AAChD,OAAO,EAAE,WAAW,EAAE,MAAM,oBAAoB,CAAA;AAGhD,SAAS,SAAS,CAAC,KAAyB;IAC1C,MAAM,OAAO,GAAG,MAAM,CAAM,IAAI,CAAC,CAAA;IACjC,MAAM,QAAQ,GAAG,MAAM,CAAM,IAAI,CAAC,CAAA;IAElC,SAAS,CAAC,GAAG,EAAE;QACb,MAAM,KAAK,GAAG,WAAW,CAAC,QAAQ,CAAC,cAAc,CAAC,OAAO,CAAgB,EAAE;YACzE,MAAM,EAAE,KAAK,CAAC,kBAAkB;YAChC,YAAY;YACZ,SAAS,EAAE;gBACT,OAAO,EAAE,KAAK;aACf;YACD,eAAe,EAAE;gBACf,OAAO,EAAE,KAAK;aACf;YACD,OAAO;YACP,SAAS,EAAE;gBACT,QAAQ,EAAE;oBACR,OAAO,EAAE,KAAK;oBACd,YAAY,EAAE,KAAK;iBACpB;gBACD,QAAQ,EAAE;oBACR,OAAO,EAAE,KAAK;oBACd,YAAY,EAAE,KAAK;iBACpB;aACF;YACD,IAAI,EAAE;gBACJ,SAAS,EAAE;oBACT,OAAO,EAAE,KAAK;iBACf;gBACD,SAAS,EAAE;oBACT,OAAO,EAAE,KAAK;iBACf;aACF;SACF,CAAC,CAAA;QAEF,MAAM,KAAK,GAAG,KAAK,CAAC,IAAI,CAAC,KAAK,CAAC,IAAI,CAAC,MAAM,GAAG,CAAC,CAAC,EAAE,KAAK,IAAI,CAAC,CAAA;QAC3D,MAAM,GAAG,GAAG,KAAK,CAAC,IAAI,CAAC,KAAK,CAAC,IAAI,CAAC,MAAM,GAAG,CAAC,CAAC,EAAE,KAAK,IAAI,CAAC,CAAA;QACzD,MAAM,IAAI,GAAG,KAAK,GAAG,GAAG,GAAG,CAAC,CAAA;QAC5B,QAAQ,CAAC,OAAO,GAAG,KAAK,CAAC,aAAa,CAAC;YACrC,SAAS,EAAE,IAAI,CAAC,CAAC,CAAC,KAAK,CAAC,WAAW,CAAC,OAAO,CAAC,CAAC,CAAC,KAAK,CAAC,WAAW,CAAC,SAAS;YACzE,QAAQ,EAAE,IAAI,CAAC,CAAC,CAAC,KAAK,CAAC,WAAW,CAAC,OAAO,CAAC,CAAC,CAAC,KAAK,CAAC,WAAW,CAAC,SAAS;YACxE,WAAW,EAAE,IAAI,CAAC,CAAC,CAAC,KAAK,CAAC,WAAW,CAAC,YAAY,CAAC,CAAC,CAAC,KAAK,CAAC,WAAW,CAAC,cAAc;YACrF,sBAAsB,EAAE,KAAK;YAC7B,g